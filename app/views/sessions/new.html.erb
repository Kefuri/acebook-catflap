<h1>Welcome back to Catflap</h1>
<h2>Please log in!</h2>

  
<%= form_for(:session, url: login_path) do |form| %> 
  <%= render 'shared/error_messages_login' %>
<p>
  <%= form.label :email %> <br>
  <%= form.email_field :email %>
</p>

<p>
  <%= form.label :password %> <br>
  <%= form.password_field :password %>
</p>

<p>
  <%= form.submit "Submit" %>
</p>
<% end %>

<div>
Not yet a member? 
<%= link_to "Sign up", '/users/new', method: :get %>
</div>
